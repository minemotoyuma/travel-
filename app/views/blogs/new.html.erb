<div class="contents row">
  <div class="container">
    <%= form_tag('/blogs', method: :post) do %>
      <h3>
        Post
      </h3>
      <input placeholder="Place" type="text" name="place" autofocus="true">
      <input placeholder="Title" type="text" name="title" autofocus="true">
      <input placeholder="Image URL" type="text" name="image_url" autofocus="true">
      <textarea placeholder="text" name="content" cols="30" rows="10"></textarea>
      <input type="submit" value="SENT">
    <% end %>
  </div>
</div>